#include<iostream>
using namespace std;
int main(){
	float a,b;
	char op;
	cout<<"Enter the operator"<<endl;
	cin>>op;
	cout<<"Enter the value of a "<<endl;
	cin>>a;
	cout<<"Enter the value of b"<<endl;
	cin>>b;
	switch(op){
		case'+':{
			cout<<a+b<<endl;
			break;
		}
			case'-':{
			cout<<a-b<<endl;
			break;
		}
			case'*':{
			cout<<a*b<<endl;
			break;
		}
			case'/':{
			cout<<a/b<<endl;
			break;
		}
		default:{
		
			cout<<"Invalid operator"<<endl;}
	}

	return 0;
